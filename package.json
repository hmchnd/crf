{
  "name": "zcrfandcompanyestimate",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.20",
    "@sap/hana-client": "^2.13.22",
    "@sap/textbundle": "^3.1.0",
    "@sap/xsenv": "^3.1.0",
    "@sap/xssec": "^3.1.2",
    "express": "4",
    "hdb": "^0.19.5",
    "passport": "^0.6.0",
    "ui5-task-zipper": "^0.5.0"
  },
  "devDependencies": {
    "@sap/approuter": "^9.4.0",
    "@sap/eslint-plugin-ui5-jsdocs": "2.0.x",
    "@sap/hdi-deploy": "^4",
    "@sap/ux-specification": "^1.108.0",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.4"
  },
  "scripts": {
    "start": "cds run",
    "watch-crf_fiori_app": "cds watch --open crf_fiori_app/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy undefined --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "engines": {
    "node": "^14.0.0",
    "npm": "^8.1.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      },
      "xsuaa": {
        "kind": "xsuaa"
      }
    }
  },
  "sapux": [
    "app/crf_fiori_app"
  ]
}